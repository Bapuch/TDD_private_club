<div class='header'>
    <nav class='navbar bg-dark text-primary navbar-fixed-top navbar-expand-md justify-content-between'>
        <ul class="navbar-nav">
          <li class="nav-item"><a class="navbar-brand " href="<%= root_path %>">
          <img src="http://icons.iconarchive.com/icons/graphicloads/colorful-long-shadow/256/Home-icon.png" width="30" height="30" class="d-inline-block align-top" alt="">
          Home
        </a></li>
          <% if logged_in? %>
            <li class="nav-item"><%= link_to 'Club', privateclub_path, class: 'nav-link'%></li>
            <li class="nav-item"><%= link_to current_user.first_name + " " + current_user.last_name, user_path(current_user.id), class: 'nav-link'%></li>
        </ul>
        <ul class='navbar-nav navbar-right'>
            <li class="nav-item"><p class='navbar-brand'>Hi <%= current_user.first_name%>!</p></li>
            <li class="nav-item"><a href="<%= logout_path %>" class="nav-link" data-confirm="Are you sure?" data-method="DELETE"><img src="https://www.iconsdb.com/icons/preview/soylent-red/logout-xxl.png" width="25" height="25" class="d-inline-block align-top" alt="log out"></a></li>
          <% else %>
            <li class="nav-item"><%= link_to 'Log in', login_path, class: 'nav-link'%></li>
            <li class="nav-item"><%= link_to 'Register', new_user_path, class: 'nav-link'%></li>
          <% end %>
        </ul>
    </nav>
</div>
